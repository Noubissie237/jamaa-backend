type Account {
  id: ID!
  userId: ID!
  accountNumber: String!
  balance: String!
  createdAt: String!
}


type Mutation {
    createAccount(userId: ID!): Account
    incrementBalance(accountId: ID!, amount: String!): Account
    decrementBalance(accountId: ID!, amount: String!): Account
}

type Query {
    getAllAccounts: [Account!]
    getAccount(id: ID!): Account
}